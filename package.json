{
  "name": "swtc-lib",
  "version": "1.4.2",
  "description": "Basic js lib to be used for interacting with jingtum blockchain network. - Keep only one websocket connecttion to jingtum and handle exception - Do transaction to jingtumd, and process response - Subscribe events, include server, ledger, account and so on - Get other information from jingtum",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "prettier": "prettier",
    "pretty-quick": "pretty-quick",
    "prettier:check": "prettier -c src/*.js srcbase/*js lib/*js",
    "prettier:tabs": "prettier --use-tabs --write src/*.js srcbase/*js lib/*js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swtcca/swtc-lib.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/swtcca/swtc-lib/issues"
  },
  "homepage": "https://github.com/swtcca/swtc-lib#readme",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --no-restage --verbose --bail"
    }
  },
  "devDependencies": {
    "husky": "^2.2.0",
    "prettier": "^1.17.0",
    "pretty-quick": "^1.10.0"
  }
}
